{
  "name": "ivy",
  "version": "1.0.0",
  "workspaces":["packages/*", "docs"],
  "private": true,
  "scripts": {
    "dev": "pnpm --filter ivy-ui run dev",
    "build": "pnpm --filter ivy-ui run build",
    "preview": "pnpm --filter ivy-ui run preview",
    "test:unit": "pnpm --filter ivy-ui run test:unit",
    "test:e2e": "pnpm --filter ivy-ui run test:e2e",
    "build-only": "pnpm --filter ivy-ui run build-only",
    "type-check": "pnpm --filter ivy-ui run type-check",
    "lint": "pnpm --filter ivy-ui run lint",
    "docs:dev": "pnpm --filter docs run dev",
    "docs:build": "pnpm --filter docs run build",
    "g": "node ./scripts/render-component.mjs"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "fs-extra": "^11.1.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "typescript": "~4.7.4"
  }
}
